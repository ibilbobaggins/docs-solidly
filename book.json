{
  "title": "Solidly V3 Documentation",
  "description": "Solidly V3 Documentation",
  "plugins": ["anchorjs", "page-toc"],
  "pluginsConfig": {
    "anchorjs": {
      "placement": "left"
    },
    "page-toc": {
      "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
      "position": "before-first",
      "showByDefault": true
    }
  }
}
